{
	"schemaVersion": 1,
	"id": "ait",
	"version": "${version}",
	"name": "Adventures In Time",
	"description": "AIT, brought to you in gleaming Fabric 1.20.1!",
	"authors": [
		"Loqor", "Duzo", "DrTheo"
	],
	"contact": {
		"homepage": "https://m-d-team.github.io",
		"sources": "https://github.com/M-D-Team/ait-fabric-1.20.1/"
	},
	"license": "CC0-1.0",
	"icon": "assets/ait/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"mdteam.ait.AITMod"
		],
		"client": [
          "mdteam.ait.client.AITModClient"
		],
		"fabric-datagen": [
          "mdteam.ait.datagen.AITModDataGenerator"
		],
		"cardinal-components-block": [
          "mdteam.ait.core.components.block.AITModBlockComponents"
		]
	},
	"mixins": [
		"ait.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": "${minecraft_version}",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-key-binding-api-v1": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"cardinal-components": [
			"ait:radionbt"
		]
	}
}